# Database settings
DATABASE_URL=sqlite:///./app.db
REDIS_URL=redis://localhost:6379/0

# LLM settings
LLM_API_KEY_SECRET_ID=sprintsense/llm/api-keys
LLM_RATE_LIMIT_PER_MINUTE=100
LLM_PROVIDERS=openai,anthropic

# Security settings
SECURITY_ALERT_CHANNELS=slack,email
JWT_SECRET_KEY=your-secret-key-here
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# AWS settings
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_DEFAULT_REGION=us-east-1

# Redis failover settings
REDIS_FALLBACK_THRESHOLD=3
REDIS_RESET_TIMEOUT=300
REDIS_CACHE_TTL=7776000  # 90 days in seconds