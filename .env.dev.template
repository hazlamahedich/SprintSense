# Development Environment Configuration
# Copy to .env.dev and fill in actual values

# Application
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG
PROJECT_NAME=SprintSense

# API Configuration
API_V1_STR=/api/v1
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:5173","http://127.0.0.1:3000","http://127.0.0.1:5173"]

# Database (Local Supabase)
POSTGRES_SERVER=localhost
POSTGRES_PORT=54322
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=postgres
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# Supabase Configuration
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your_local_anon_key_here
SUPABASE_SERVICE_KEY=your_local_service_key_here

# Frontend
VITE_API_URL=http://localhost:8000
VITE_SUPABASE_URL=http://127.0.0.1:54321
VITE_SUPABASE_ANON_KEY=your_local_anon_key_here

# Optional Services
SENTRY_DSN=
SLACK_WEBHOOK_URL=

# Testing
TEST_DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres_test
